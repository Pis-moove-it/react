{
  "name": "reciclando-web",
  "version": "1.0.0",
  "description": "Reciclando Web application",
  "repository": {
    "type": "git",
    "url": "https://github.com/Pis-moove-it/reciclando-web"
  },
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config webpack.dev.config.js",
    "lint": "eslint --fix . --ext .js || exit 0",
    "build": "webpack -p --config ./webpack.build.config.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "author": "Pis-moove-it",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.7.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-enzyme": "^6.0.4",
    "style-loader": "^0.21.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-hot-loader": "^4.3.8",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.9",
    "url-loader": "^1.1.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./__tests__/setup/setupEnzyme.js",
    "testPathIgnorePatterns": [
      "./__tests__/setup/",
      "!./__tests__/"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/__tests__/**",
      "!**/coverage/**",
      "!**/public/**",
      "!**/.vscode/**",
      "!**/webpack.**",
      "!**/index.js"
    ]
  }
}
